datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Role {
  id Int @id @default(autoincrement())
  active Boolean @default(true)
  name String
  description String?
  users User[]  
}

model User {
  id Int @id @default(autoincrement())
  email String  @unique
  name  String?
  phone  String?
  role  Role? @relation(fields: [roleId], references: [id])
  roleId Int?
}
